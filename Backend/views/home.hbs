<table class="table table-bordered table-dark">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">Email</th>
      <th scope="col"> Name</th>
      <th scope="col">Password</th>
      <th scope="col">IsAdmin</th>
      <th scope="col" class="text-end">Action</th>

    </tr>
  </thead>
  <tbody>
    {{#each rows}}
      <tr>
        <th scope="row">{{this.id}}</th>
        <td>{{this.email}}</td>
        <td>{{this.name}}</td>
        <td>{{this.password}}</td>
        <td>{{this.isAdmin}}</td>
        <td class="text-end">
          <a href="#" type="button" class="btn btn-dark btn-small"><i
              class="bi bi-eye"
            ></i>View</a>
          <a href="#" type="button" class="btn btn-dark btn-small"><i
              class="bi bi-pencil"
            ></i>Edit</a>
          <a href="#" type="button" class="btn btn-dark btn-small"><i
              class="bi bi-person-x"
            ></i>Delete</a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>